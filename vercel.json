{
  "version": 2,
  "name": "shamsi-calendar-api",
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": [
    "iad1"
  ],
  "functions": {
    "src/app.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}